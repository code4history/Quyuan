{
  "name": "@c4h/quyuan",
  "version": "0.3.0",
  "description": "GeoJSON template extractor and multimedia viewer",
  "type": "module",
  "private": false,
  "main": "./dist/quyuan.umd.js",
  "module": "./dist/quyuan.js",
  "browser": "./dist/quyuan.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/quyuan.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "npm run typecheck && cross-env BUILD_MODE=package vite build",
    "build:demo": "vite build",
    "deploy": "npm run typecheck && vite build",
    "typecheck": "tsc --noEmit --allowImportingTsExtensions",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "playwright test --config=playwright-ci.config.ts",
    "lint": "eslint src tests",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "dependencies": {
    "@c4h/chuci": "github:code4history/Chuci#c2839bdbee2cee555f1866147b240766ed03f194",
    "lit": "^3.3.1",
    "nunjucks": "^3.2.4",
    "uevent": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/geojson": "^7946.0.16",
    "@types/jsdom": "^21.1.7",
    "@types/leaflet": "^1.9.21",
    "@types/nunjucks": "^3.2.6",
    "@types/three": "^0.171.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "jsdom": "^25.0.1",
    "leaflet": "^1.9.4",
    "maplibre-gl": "^5.15.0",
    "ol": "^10.7.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "geojson",
    "template",
    "viewer",
    "webcomponents"
  ],
  "author": "Code for History",
  "contributors": [
    "kobakazu0429 <kobakazu10@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/Quyuan.git"
  },
  "bugs": {
    "url": "https://github.com/code4history/Quyuan/issues"
  },
  "homepage": "https://github.com/code4history/Quyuan#readme"
}